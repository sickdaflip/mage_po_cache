            <processor>Potato_FullPageCache_Model_Processor_Block_Session_Wishlist</processor>
        </wishlist_sidebar>
        <global_cookie_notice/>
    </session_blocks>
    <dynamic_blocks>
        <AW_Countdown_Block_Countdown>
            <processor>Potato_FullPageCache_Model_Processor_Block_Dynamic_Countdown</processor>
        </AW_Countdown_Block_Countdown>
    </dynamic_blocks>
    <allowed_routers>
        <catalog>
            <controllers>
                <category>view</category>
                <product>view</product>
            </controllers>
            <parameters>
                <processor>po_fpc/processor_router_catalog</processor>
                <lifetime>2592000</lifetime>
                <skip_blocks/>
            </parameters>
        </catalog>
        <cms>
            <controllers>
                <page>*</page>
                <index>*</index>
            </controllers>
            <parameters>
                <skip_blocks/>
                <lifetime>2592000</lifetime>
            </parameters>
        </cms>
        <wordpress>
            <controllers>
                <post>*</post>
                <view>*</view>
            </controllers>
            <parameters>
                <skip_blocks/>
                <lifetime>2592000</lifetime>
            </parameters>
        </wordpress>
    </allowed_routers>
</config>