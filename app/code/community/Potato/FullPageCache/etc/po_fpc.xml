        </catalog_compare_sidebar>
        <minicart_head>
            <processor>Potato_FullPageCache_Model_Processor_Block_Session_Cart</processor>
        </minicart_head>
        <right_poll>
            <name_in_layout>right.poll</name_in_layout>
        </right_poll>
        <right_reports_product_viewed>
            <name_in_layout>right.reports.product.viewed</name_in_layout>
            <processor>Potato_FullPageCache_Model_Processor_Block_Session_Viewed</processor>
        </right_reports_product_viewed>
        <left_reports_product_viewed>
